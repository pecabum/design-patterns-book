public class Cutter {

	public void turnOn() {
		System.out.println("Включване на машина за рязане");
	}

	public void turnOff() {
		System.out.println("Изключване на машина за рязане");
	}

	public void cutDough() {
		System.out.println("Рязане на тесто");
	}
	
	public void cutFruits() {
		System.out.println("Рязане на плодове");
	}
}
