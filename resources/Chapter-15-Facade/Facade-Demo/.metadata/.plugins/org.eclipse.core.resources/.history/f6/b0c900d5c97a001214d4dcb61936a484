
public class Robot {

	public void turnOn() {
		System.out.println("Включване на робот");
	}

	public void turnOff() {
		System.out.println("Изключване на робот");
	}
	
	public void move(String material, String source, String destination) {
		System.out.println("Преместване на " + material + " от " + source + " в " + destination);
	}
}
