
public class Packer {

	public void turnOn() {
		System.out.println("Включване на опаковъчна машина");
	}

	public void turnOff() {
		System.out.println("Изключване на опаковъчна машина");
	}
	
	public void packBread() {
		System.out.println("Опаковане на хляб");
	}
	
	public void packDough() {
		System.out.println("Опаковане на тесто");
	}
}
